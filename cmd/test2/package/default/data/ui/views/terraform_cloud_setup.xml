<!--
Both of the <dashboard> element's `styles` and `script` attribute search for
files in `$SPLUNK_HOME/etc/<SPLUNK_APP_NAME>/appserver/static/*`

Documentation link:
http://docs.splunk.com/Documentation/Splunk/latest/Viz/PanelreferenceforSimplifiedXML#dashboard_or_form
-->
<dashboard hideAppBar="true"
           hideEdit="true"
           hideTitle="true"
           isVisible="false"
           hideExport="true"
           hideSplunkBar="false"
           isDashboard="true"
           script="javascript/terraform_cloud_setup.js"
           stylesheet="styles/tfcloud.css">
    <row>
        <panel>
            <html>
                <div id="main_container"></div>
            </html>
        </panel>
    </row>
</dashboard>
